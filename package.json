{
	"name": "schema-cdegen-test",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"codegen:codec-hospital": "node src/codec-builder-library/codegen/generate-codec-output-schema.mjs --entry src/consumer-code/batteries/batteries.ts --export HospitalServerToFormCodec --schema HospitalFormSchema --out src/consumer-code/batteries/generated/hospital/reshape-schema.ts",
		"check:codec-hospital": "bun run scripts/check-codec-hospital.ts",
		"codegen:codec-school": "node src/codec-builder-library/codegen/generate-codec-output-schema.mjs --entry src/consumer-code/batteries/batteries.ts --export SchoolServerToFormCodec --schema SchoolFormSchema --out src/consumer-code/batteries/generated/school/reshape-schema.ts",
		"check:codec-school": "bun run scripts/check-codec-school.ts",
		"dev": "next dev --turbo",
		"preview": "next build && next start",
		"start": "next start",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.2",
		"@t3-oss/env-nextjs": "^0.12.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.563.0",
		"next": "^15.2.3",
		"radix-ui": "^1.4.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.71.1",
		"tailwind-merge": "^3.4.0",
		"zod": "3.25.48"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@tailwindcss/postcss": "^4.0.15",
		"@types/node": "^20.14.10",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"postcss": "^8.5.3",
		"shadcn": "^3.8.4",
		"tailwindcss": "^4.0.15",
		"tw-animate-css": "^1.4.0",
		"typescript": "5.3.3"
	},
	"ct3aMetadata": {
		"initVersion": "7.40.0"
	}
